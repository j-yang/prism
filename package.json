{
  "name": "prism-platform",
  "version": "2.0.0",
  "description": "PRISM - Platform for Research Infrastructure Smart Manufacturing",
  "main": "src/App.vue",
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "vite",
    "dev:server": "node server/server.js",
    "build": "vite build",
    "build-with-type-check": "vue-tsc && vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "deploy": "npm run build && gh-pages -d dist -r https://github.com/azu-oncology-rd/prism.git -b gh-pages"
  },
  "dependencies": {
    "file-saver": "^2.0.5",
    "jszip": "^3.10.1",
    "pinia": "^3.0.3",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.7",
    "@types/jszip": "^3.4.1",
    "@types/node": "^24.1.0",
    "@vitejs/plugin-vue": "^5.2.0",
    "@vue/tsconfig": "^0.5.1",
    "concurrently": "^9.1.0",
    "gh-pages": "^6.2.0",
    "typescript": "^5.7.2",
    "vite": "^5.4.0",
    "vue-tsc": "^2.2.12"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "overrides": {
    "typescript": "^5.7.2"
  }
}
